{
    "name": "sourcefli/collection-macros",
    "description": "Useful Collection macros I find myself using in different projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Sourcefli",
            "email": "mail@jhavens.tech",
            "homepage": "https://jhavens.tech"
        }
    ],
    "homepage": "https://github.com/sourcefli/collection-macros",
    "keywords": ["Laravel", "CollectionMacros"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Sourcefli\\CollectionMacros\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sourcefli\\CollectionMacros\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sourcefli\\CollectionMacros\\CollectionMacrosServiceProvider"
            ],
            "aliases": {
                "CollectionMacros": "Sourcefli\\CollectionMacros\\Facades\\CollectionMacros"
            }
        }
    }
}
